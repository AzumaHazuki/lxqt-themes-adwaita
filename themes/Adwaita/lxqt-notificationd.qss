Notification {
    color: #000000;
    border: 1px solid #cdc7c2;
    border-radius: 4px;
    background: #f6f5f4;
    margin: 0px;
}

QLabel {
    color: #000000;
}

#closeButton {
    border: none;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#closeButton:hover {
    border: 1px solid #cdc7c2;
    border-radius: 4px;
    background: #f8f8f7;
    qproperty-icon: url(lxqt-notificationd/cancel.svg);
}

#appLabel {
    font-weight: bold;
}
